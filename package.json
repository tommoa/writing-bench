    "@ai-sdk/openai-compatible": "^2.0.30",
    "ai": "^6.0.0",
    "ink": "^5.1.0",
    "ink-spinner": "^5.0.0",
    "nanoid": "^5.1.0",
    "p-queue": "^8.1.0",
    "react": "^18.3.1",
    "smol-toml": "^1.3.1",
    "yargs": "^17.7.2",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/bun": "^1.2.0",
    "@types/react": "^18.3.0",
    "@types/yargs": "^17.0.33",
