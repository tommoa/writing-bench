{
  "name": "writing-bench",
  "version": "0.1.0",
  "description": "LLM writing quality benchmark with ELO scoring",
  "type": "module",
  "bin": {
    "writing-bench": "./src/index.tsx"
  },
  "scripts": {
    "start": "bun run src/index.tsx",
